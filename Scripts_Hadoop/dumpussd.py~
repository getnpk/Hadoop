import os
import sys
import pexpect

#python setup.py install -- installing pexpect package.

try :
    foo5=pexpect.spawn("scp -r root@10.0.9.223:/mysql/backup/USSD_CSV_New_20  .")
    foo5.expect('.ssword:*')
    foo5.sendline('redhat')
    foo5.interact()
    foo5.close()
except Exception,e:
    pass

