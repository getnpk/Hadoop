    f.write('line two\n')
    f.writelines('line therr')
    f.close

    f = open('test.txt','r')

    while True:
        line = f.readline()
        if len(line) == 0:
            break
        print(line, end='')
    f.close()

    f = open('test.txt','r')
    m=f.readlines()
    m.sort()
    print(m)
    f.close

    f = open('test.txt','r')
    k = f.read()
    l = k.split()
    print(len(l))

    url = 'http://xml.resource.org/public/rfc/txt/rfc793.txt'
    destination_filename = 'rfc793.txt'
    url='https://www.google.co.in'
    f='photo.jpg'
